{
  "name": "@cele/nx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "@cele/nx/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/@cele/nx",
        "main": "@cele/nx/src/index.ts",
        "tsConfig": "@cele/nx/tsconfig.lib.json",
        "assets": [
          "@cele/nx/*.md",
          {
            "input": "./@cele/nx/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./@cele/nx/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./@cele/nx",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./@cele/nx",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    }
  }
}
