{
  "$schema": "https://json-schema.org/schema",
  "$id": "Celement",
  "title": "Generates a new CElement in your project",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name (kebab-case) of the CElement",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    }
  },
  "required": ["name"]
}
